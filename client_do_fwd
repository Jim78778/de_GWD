/usr/local/bin/ui-markThis
fi

echo "" > /usr/local/bin/ui-aclifwd
}


start_menu(){
    green "================================"
    green "      CLIENT_fwd ( Docker )"
    green "Recommend: Debian 9 + ( amd64 )"
    green "Author:    JacyL4"
    green "================================"
    echo
    green  "1. de_GWD clifwd install"
    green  "2. Install bbrplus and reboot（only Debian 9）"
    green  "6. make SSL"
    green  "9. Change de_GWD password"
    yellow "0. de_GWD clifwd Update"
    red    "CTRL+C EXIT"
    echo
    read -p "Select:" num
    case "$num" in
    1)
    installClient_do_fwd
    start_menu
    ;;
    2)
    installBBRplus
    start_menu
    ;;
    6)
    makeSSL
    start_menu
    ;;
    9)
    change_piholeAdmin
    start_menu
    ;;
    0)
    clifwdUpdate
    start_menu
    ;;
    *)
    clear
    red "Wrong number"
    sleep 1s
    start_menu
    ;;
    esac
}

start_menu